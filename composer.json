{
	"name": "vance-page/laravel-notification-easysms",
	"description": "EasySms Notification Channel for Laravel.",
	"keywords": ["laravel", "notifications", "easysms"],
	"type": "library",
	"license": "MIT",
    "authors": [
        {
            "name": "vance",
            "homepage": "https://github.com/vance-page"
        }
    ],
	"require": {
		"overtrue/easy-sms": "^2.0"
	},
	"autoload": {
		"psr-4": {
			"Vance\\LaravelNotificationEasySms\\": "src"
		}
	},
	"config": {
		"sort-packages": true
	},
	"extra": {
		"laravel": {
			"providers": [
				"Vance\\LaravelNotificationEasySms\\EasySmsChannelServiceProvider"
			]
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
